function eachCharCount(String)
{
    let counts={}
    for(let i=0;i<String.length;i++)
    {
        let char=String.charAt(i)
        if(counts[char])
        {
            counts[char]++
        }
        else{
            counts[char]=1;
        }
    }
    return counts;
}
const Str="minnu";
console.log(eachCharCount(Str))